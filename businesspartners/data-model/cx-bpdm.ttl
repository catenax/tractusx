###  http://catena-x.net/bpdm#AddressType
:AddressType rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Class ;
                                   owl:oneOf ( :BRANCH_OFFICE
                                               :CARE_OF
                                               :HEADQUARTER
                                               :LEGAL
                                               :LEGAL_ALTERNATIVE
                                               :PO_BOX
                                               :REGISTERED_ADDRESS
                                               :REGISTERED_AGENT_MAIL
                                               :REGISTERED_AGENT_PHYSICAL
                                               :UNSPECIFIC
                                               :VAT_REGISTERED_ADDRESS
                                             )
                                 ] .


###  http://catena-x.net/bpdm#AddressVersion
:AddressVersion rdf:type owl:Class ;
                rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                         owl:onProperty :hasElement ;
                                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                         owl:onClass :CharacterSet
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :hasLanguage ;
                                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                         owl:onDataRange xsd:language
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] .


###  http://catena-x.net/bpdm#AdministrativeArea
:AdministrativeArea rdf:type owl:Class ;
                    rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasElement ;
                                                             owl:someValuesFrom :AdministrativeAreaCode
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasElement ;
                                                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                             owl:onClass :AdministrativeAreaType
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasValue ;
                                                             owl:minCardinality "1"^^xsd:nonNegativeInteger
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] .


###  http://catena-x.net/bpdm#AdministrativeAreaCode
:AdministrativeAreaCode rdf:type owl:Class ;
                        rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :hasElement ;
                                                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                 owl:onClass :GenericType
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :hasValue ;
                                                                 owl:cardinality "1"^^xsd:nonNegativeInteger
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] .


###  http://catena-x.net/bpdm#AdministrativeAreaType
:AdministrativeAreaType rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Class ;
                                              owl:oneOf ( :COUNTY
                                                          :OTHER_AAT
                                                          :REGION
                                                        )
                                            ] .


###  http://catena-x.net/bpdm#BankAccount
:BankAccount rdf:type owl:Class ;
             rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasElement ;
                                                      owl:someValuesFrom :TrustScore
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasElement ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass :Currency
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasElement ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass :InternationalBankAccountIdentifier
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasElement ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass :InternationalBankIdentifier
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasElement ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass :NationalBankAccountIdentifier
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasElement ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass :NationalBankIdentifier
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] .


###  http://catena-x.net/bpdm#BusinessPartner
:BusinessPartner rdf:type owl:Class ;
                 rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasElement ;
                                                          owl:someValuesFrom :Address
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasElement ;
                                                          owl:someValuesFrom :BankAccount
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasElement ;
                                                          owl:someValuesFrom :BusinessPartnerRelation
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasElement ;
                                                          owl:someValuesFrom :BusinessPartnerType
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasElement ;
                                                          owl:someValuesFrom :LegalForm
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasElement ;
                                                          owl:someValuesFrom :Name
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasRole ;
                                                          owl:someValuesFrom :Role
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasElement ;
                                                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :Identifier
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasElement ;
                                                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :BusinessPartnerProfile
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasElement ;
                                                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :BusinessPartnerStatus
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] .


