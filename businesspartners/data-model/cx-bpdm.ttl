<http://catena-x.net/bpdm> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://catena-x.net/bpdm#hasElement
:hasElement rdf:type owl:ObjectProperty ;
.


###  http://catena-x.net/bpdm#hasEndNode
:hasEndNode rdf:type owl:ObjectProperty ;
            rdfs:range :BusinessPartner .


###  http://catena-x.net/bpdm#hasRole
:hasRole rdf:type owl:ObjectProperty ;
         rdfs:domain :Role .


###  http://catena-x.net/bpdm#hasStartNode
:hasStartNode rdf:type owl:ObjectProperty ;
              rdfs:range :BusinessPartner .


#################################################################
#    Data properties
#################################################################

###  http://catena-x.net/bpdm#endedAt
:endedAt rdf:type owl:DatatypeProperty ;
         rdfs:range xsd:dateTimeStamp .


###  http://catena-x.net/bpdm#hasLanguage
:hasLanguage rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:language .


###  http://catena-x.net/bpdm#hasNumber
:hasNumber rdf:type owl:DatatypeProperty ;
           rdfs:range xsd:integer .


###  http://catena-x.net/bpdm#hasShortName
:hasShortName rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:string .


###  http://catena-x.net/bpdm#hasValue
:hasValue rdf:type owl:DatatypeProperty ;
          rdfs:range rdfs:Literal .


###  http://catena-x.net/bpdm#initialRegistration
:initialRegistration rdf:type owl:DatatypeProperty ;
                     rdfs:range xsd:dateTimeStamp .


###  http://catena-x.net/bpdm#lastUpdate
:lastUpdate rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:dateTimeStamp .


###  http://catena-x.net/bpdm#startedAt
:startedAt rdf:type owl:DatatypeProperty ;
           rdfs:range xsd:dateTimeStamp .


