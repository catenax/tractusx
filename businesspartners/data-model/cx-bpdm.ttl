###  http://catena-x.net/bpdm#Currency
:Currency rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hasElement ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :GenericType
                          ] .


###  http://catena-x.net/bpdm#DataElement
:DataElement rdf:type owl:Class ;
             rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasValue ;
                                                      owl:cardinality "1"^^xsd:nonNegativeInteger
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasNumber ;
                                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onDataRange xsd:integer
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasShortName ;
                                                      owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] .


###  http://catena-x.net/bpdm#GenericType
:GenericType rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :hasValue ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] ;
             rdfs:comment "Provides a string value to identify the type of a data element." .


###  http://catena-x.net/bpdm#HardeningGrade
