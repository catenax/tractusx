###  http://catena-x.net/bpdm#BusinessPartnerProfile
:BusinessPartnerProfile rdf:type owl:Class ;
                        rdfs:subClassOf [ rdf:type owl:Restriction ;
                                          owl:onProperty :hasElement ;
                                          owl:someValuesFrom :BusinessPartnerClassification
                                        ] .


###  http://catena-x.net/bpdm#BusinessPartnerRelation
:BusinessPartnerRelation rdf:type owl:Class ;
                         rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :hasElement ;
                                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                  owl:onClass :RelationClass
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :hasElement ;
                                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                  owl:onClass :RelationType
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :hasEndNode ;
                                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                  owl:onClass :BusinessPartner
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :hasStartNode ;
                                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                  owl:onClass :BusinessPartner
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :endedAt ;
                                                                  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                  owl:onDataRange xsd:dateTimeStamp
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :startedAt ;
                                                                  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                  owl:onDataRange xsd:dateTimeStamp
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] .


