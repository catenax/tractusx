                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :lastUpdate ;
                                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onDataRange xsd:dateTimeStamp
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] .


###  http://catena-x.net/bpdm#RegistrationStatus
:RegistrationStatus rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Class ;
                                          owl:oneOf ( :ISSUED
                                                      :RELEASED
                                                      :RETIRED
                                                    )
                                        ] .


###  http://catena-x.net/bpdm#RelationClass
:RelationClass rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Class ;
                                     owl:oneOf ( :CDQ_HIERARCHY
                                                 :CDQ_TRANSITION
                                                 :CX_HIERARCHY
                                                 :DNB_HIERARCHY
                                                 :LEI_HIERARCHY
                                               )
                                   ] .


###  http://catena-x.net/bpdm#RelationType
:RelationType rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Class ;
                                    owl:oneOf ( :COMMERCIAL_ULTIMATE
                                                :CX_ADDRESS_OF
                                                :CX_LEGAL_PREDECESSOR_OF
                                                :CX_LEGAL_SUCCESSOR_OF
                                                :CX_OWNED_BY
                                                :CX_SITE_OF
                                                :DIRECT_LEGAL_RELATION
                                                :DNB_DOMESTIC_ULTIMATE
                                                :DNB_GLOBAL_ULTIMATE
                                                :DNB_HEADQUARTER
                                                :DNB_PARENT
                                                :DOMESTIC_BRANCH_RELATION
                                                :DOMESTIC_LEGAL_ULTIMATE_RELATION
                                                :GLOBAL_LEGAL_ULTIMATE_RELATION
                                                :INTERNATIONAL_BRANCH_RELATION
                                                :LEGAL_PREDECESSOR
                                                :LEGAL_SUCCESSOR
                                                :LEI_DIRECT_PARENT
                                                :LEI_INTERNATIONAL_BRANCH
                                                :LEI_ULTIMATE_PARENT
                                              )
                                  ] .


###  http://catena-x.net/bpdm#Role
:Role rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :hasElement ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :GenericType
                      ] .


###  http://catena-x.net/bpdm#Thoroughfare
:Thoroughfare rdf:type owl:Class ;
              rdfs:subClassOf [ owl:intersectionOf ( :DataElement
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasElement ;
                                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                       owl:onClass :ThoroughfareType
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] .


###  http://catena-x.net/bpdm#ThoroughfareType
:ThoroughfareType rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Class ;
                                        owl:oneOf ( :INDUSTRIAL_ZONE
                                                    :OTHER_TFT
                                                    :RIVER
                                                    :SQUARE
                                                    :STREET
                                                  )
                                      ] .


###  http://catena-x.net/bpdm#TrustScore
:TrustScore rdf:type owl:Class ;
            rdfs:comment "Business definition pending..." .


