    <!-- http://catena-x.net/bpdm#Identifier -->

    <owl:Class rdf:about="http://catena-x.net/bpdm#Identifier">
        <rdfs:subClassOf rdf:resource="http://catena-x.net/bpdm#DataElement"/>
