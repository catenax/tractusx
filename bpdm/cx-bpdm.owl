                    <rdf:Description rdf:about="http://catena-x.net/bpdm#LEGAL"/>
                    <rdf:Description rdf:about="http://catena-x.net/bpdm#LEGAL_ALTERNATIVE"/>
                    <rdf:Description rdf:about="http://catena-x.net/bpdm#PO_BOX"/>
                    <rdf:Description rdf:about="http://catena-x.net/bpdm#REGISTERED_ADDRESS"/>
                    <rdf:Description rdf:about="http://catena-x.net/bpdm#REGISTERED_AGENT_MAIL"/>
                    <rdf:Description rdf:about="http://catena-x.net/bpdm#REGISTERED_AGENT_PHYSICAL"/>
                    <rdf:Description rdf:about="http://catena-x.net/bpdm#UNSPECIFIC"/>
                    <rdf:Description rdf:about="http://catena-x.net/bpdm#VAT_REGISTERED_ADDRESS"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


