<?xml version="1.0"?>
<rdf:RDF xmlns="http://catena-x.net/bpdm#"
     xml:base="http://catena-x.net/bpdm"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://catena-x.net/bpdm"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://catena-x.net/bpdm#hasElement -->

    <owl:ObjectProperty rdf:about="http://catena-x.net/bpdm#hasElement"/>
    


    <!-- http://catena-x.net/bpdm#hasEndNode -->

    <owl:ObjectProperty rdf:about="http://catena-x.net/bpdm#hasEndNode"/>
    


    <!-- http://catena-x.net/bpdm#hasStartNode -->

    <owl:ObjectProperty rdf:about="http://catena-x.net/bpdm#hasStartNode"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://catena-x.net/bpdm#hasLanguage -->

    <owl:DatatypeProperty rdf:about="http://catena-x.net/bpdm#hasLanguage">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#language"/>
        <rdfs:comment>Lower case two-letter codes for language names according to RFC 1766 language codes.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://catena-x.net/bpdm#hasNumber -->

    <owl:DatatypeProperty rdf:about="http://catena-x.net/bpdm#hasNumber">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://catena-x.net/bpdm#hasShortName -->

    <owl:DatatypeProperty rdf:about="http://catena-x.net/bpdm#hasShortName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://catena-x.net/bpdm#hasValue -->

    <owl:DatatypeProperty rdf:about="http://catena-x.net/bpdm#hasValue">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


