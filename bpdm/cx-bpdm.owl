    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://catena-x.net/bpdm#Address -->

    <owl:Class rdf:about="http://catena-x.net/bpdm#Address">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://catena-x.net/bpdm#hasElement"/>
                        <owl:someValuesFrom rdf:resource="http://catena-x.net/bpdm#AdministrativeArea"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://catena-x.net/bpdm#hasElement"/>
                        <owl:someValuesFrom rdf:resource="http://catena-x.net/bpdm#Locality"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://catena-x.net/bpdm#hasElement"/>
                        <owl:someValuesFrom rdf:resource="http://catena-x.net/bpdm#PostCode"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://catena-x.net/bpdm#hasElement"/>
                        <owl:someValuesFrom rdf:resource="http://catena-x.net/bpdm#PostalDeliveryPoint"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://catena-x.net/bpdm#hasElement"/>
                        <owl:someValuesFrom rdf:resource="http://catena-x.net/bpdm#Premise"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://catena-x.net/bpdm#hasElement"/>
                        <owl:someValuesFrom rdf:resource="http://catena-x.net/bpdm#Thoroughfare"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://catena-x.net/bpdm#hasElement"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                        <owl:onClass rdf:resource="http://catena-x.net/bpdm#AddressType"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://catena-x.net/bpdm#hasElement"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                        <owl:onClass rdf:resource="http://catena-x.net/bpdm#Country"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
    </owl:Class>
    


